<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.everis.listadecontatos">

    <application
        android:icon="@mipmap/app_ic_contact"
        android:roundIcon="@mipmap/app_ic_contact"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:name=".application.ContactsApplication"
        android:theme="@style/AppTheme"
        android:allowBackup="true">
        <activity android:name=".feature.contato.ContactActivity" />
        <activity android:name=".feature.listacontatos.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>